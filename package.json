{
    "name": "honkit-plugin-robots",
    "description": "Generate a robots.txt for a Honkit",
    "main": "index.js",
    "version": "1.0.1",
    "engines": {
        "honkit": ">=1.x.x"
    },
    "homepage": "https://github.com/stuyk/honkit-plugin-robots",
    "keywords": [
        "seo",
        "robots",
        "honkit"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/stuyk/honkit-plugin-robots"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/stuyk/honkit-plugin-robots/issues"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": true
    },
    "honkit": {
        "properties": {
            "data": {
                "type": "[]",
                "required": true,
                "title": "A list of data to write down to a robot.txt file. Each array element is a line."
            }
        }
    }
}
